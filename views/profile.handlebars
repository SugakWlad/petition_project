<form method="get" action="/logout">
	<button id="log" class="headerbt">Log Out</button>
</form>  
<h1>We would like to know more about you</h1>
<form id="profile" method="post" action="/profile">
	<div class="regdiv">
		<h3>Age</h3>
		<input name="age">
		<h3>City</h3>
		<input type="text" name="city">
		<h3>Homepage</h3>
		<input type="url" name="homepage" value="">
		<input type="hidden" name="_csrf" value="{{csrfToken}}">
		<button class="sign">CONTINUE</button>
	</div>
</form>